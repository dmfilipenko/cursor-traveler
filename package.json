{
  "name": "cursor-traveler",
  "version": "3.0.0",
  "description": "Cursor Traveler: Chrome Extension. Count how many moves make your mouse",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "zip": "zip -r -X cursor-traveler.zip dist",
    "build:zip": "npm run build && npm run zip",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "author": "Dmytro Filipenko",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmfilipenko/cursor-traveler"
  },
  "dependencies": {
    "@effect/platform": "^0.87.1",
    "@effect/platform-browser": "^0.67.1",
    "big.js": "^7.0.1",
    "effect": "^3.16.10"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0",
    "@types/big.js": "^6.2.2",
    "@types/chrome": "^0.0.270",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jsdom": "^26.1.0",
    "prettier": "^3.1.1",
    "terser": "^5.43.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}
